# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_JWT_SECRET=your-jwt-secret
DATABASE_URL=postgresql://user:pass@host:6543/db

# OpenAI
OPENAI_API_KEY=sk-...
# SII FAQ Vector Store ID for file search (get from vector_stores.json)
SII_FAQ_VECTOR_STORE_ID=vs_xxx

# Kapso WhatsApp
KAPSO_API_KEY=your-kapso-api-key
KAPSO_PROJECT_ID=your-project-id
KAPSO_WHATSAPP_CONFIG_ID=your-whatsapp-config-id

# Redis (Celery)
REDIS_URL=redis://localhost:6379/0

# Encryption
ENCRYPTION_KEY=your-32-byte-encryption-key

# Environment
ENVIRONMENT=development

# Seed Scripts - Database URLs for different environments
# Used by scripts/seed for syncing data between environments
DATABASE_URL=postgresql://user:pass@localhost:6543/fizko_local
STAGING_DATABASE_URL=postgresql://user:pass@staging-host:5432/fizko_staging
DATABASE_URL_PRODUCTION=postgresql://user:pass@prod-host:5432/fizko_production
